package jan13;

import java.util.Scanner;

public class UniqueNumber {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner scn=new Scanner(System.in);
		int n=scn.nextInt();
		int[] arr={1,2,3,4,3,2,1};
//		for(int i=0;i<n;i++)
//		{
//			arr[i]=scn.nextInt();
//		}
		findoneuniquenumber(arr);
		int[] arr1={1,2,3,4,5,3,2,1};
		findtwouniquenumber(arr1);
	}
	public static void findoneuniquenumber(int[] arr)
	{
		int res=0;
			for(int i=1;i<arr.length;i++)
		{
			res^=i; //^ converts number into bit and gives out a new number 
		}
			System.out.println(res);
	}
	public static void findtwouniquenumber(int[] arr)
	{
		int res=0;
		for(int i=1;i<arr.length;i++)
	{
		res^=i; //^ converts number into bit and gives out a new number 
	}
		System.out.println(res);
	}
}
