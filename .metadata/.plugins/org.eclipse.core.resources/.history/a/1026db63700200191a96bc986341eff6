package dec17;

import java.util.Scanner;

public class revereseprint {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner n=new Scanner(System.in);
		int i;
		int j;
		for(i=1;i<=n;i++)
		{
			for(j=1;j<=i;j++)
			{
				System.out.println("*");
			}
		}
	}

}
