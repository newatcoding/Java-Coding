package jan12;

public class Sudoku {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[][] mat = { { 3, 0, 6, 5, 0, 8, 4, 0, 0 },
				{ 5, 2, 0, 0, 0, 0, 0, 0, 0 },
				{ 0, 8, 7, 0, 0, 0, 0, 3, 1 },
				{ 0, 0, 3, 0, 1, 0, 0, 8, 0 }, 
				{ 9, 0, 0, 8, 6, 3, 0, 0, 5 },
				{ 0, 5, 0, 0, 9, 0, 6, 0, 0 },
				{ 1, 3, 0, 0, 0, 0, 2, 5, 0 },
				{ 0, 0, 0, 0, 0, 0, 0, 7, 4 },
				{ 0, 0, 5, 2, 0, 6, 3, 0, 0 } };
		Sudoku(mat);
	}
	public static void Sudoku(int[][] mat)
	{
//		System.out.println(mat.length+"   "+mat[0].length);
		for(int i=0;i<mat.length;i++)
		{
		for(int j=0;j<mat[0].length;j++)
		{
			if(mat[i][j]==0)
			{
				System.out.print("_ ");
			}
			else if(mat[i][j]>0)
			{
			System.out.print(mat[i][j]+" ");
			}
			if(j==2 || j==5)
			{
				System.out.print("|");
			}
			
		}
		System.out.println(" |");
		if(i==2 || i==5)
		{
			System.out.println("_ _ _ _ _ _ _ _ _ _ _ _ _");
		}
		}
		System.out.println("_ _ _ _ _ _ _ _ _ _ _ _ _");
	}

}
