package page8;

import java.util.Scanner;

public class SelectionSort {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner scn=new Scanner(System.in);
		int n=scn.nextInt();
		int[] arr=new int[n];
		for(int i=0;i<n;i++){
			arr[i]=scn.nextInt();
		}
		SelectionSort(arr,0,0,n,arr[0]);
		for(int j=0;j<n;j++)
		{
			System.out.print(arr[j]+" ");
		}
	}
	
	public static void SelectionSort(int[] arr,int pos,int find,int n,int min){
		if(pos==n)
		{
			return;
		}
		if(find==n)
		{
			return;
		}
		int p=pos;
		if(min>arr[find])
		{
			p=find;
			min=arr[find];
		}
		SelectionSort(arr, pos, find+1, n, min);
		System.out.println(" ");
		arr[p]=arr[pos];
		arr[pos]=min;
		min=arr[pos+1];
		find =0;
		SelectionSort(arr, pos+1, find, n, min);
	}


}
