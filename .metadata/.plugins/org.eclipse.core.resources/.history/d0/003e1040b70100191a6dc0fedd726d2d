package dec17;

public class comparison {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a=12;
		int b=23;
		int c=34;
		if(a>b && a>c )
			{
			System.out.println(a+"a is the greatest" );
			}
		else if(b>c)
		{
			System.out.println(b+"is the greatest");
		}
		else
		{
			System.out.println(c+"is the greatest");
		}
			
	}

}
