package page5;

import java.util.Scanner;

public class Sorting {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner scn=new Scanner(System.in);
		int N=scn.nextInt();
		int[] arr1 = new int[N];
		int[] arr2 = new int[N];
		int[] arr3 = new int[N];
		for (int i = 0; i < N; i++) {
			arr1[i] = scn.nextInt();
		}
		for (int i = 0; i < N; i++) {
			arr2[i] = scn.nextInt();
		}
		for (int i = 0; i < N; i++) {
			arr3[i] = scn.nextInt();
		}
		bubblesort(arr1);
		insersionsort(arr2);
		selectionsort(arr3);
	}
	public static void insersionsort(int[] arr)
	{
		int c;
		for(int i=0;i<arr.length;i++)
		{
			for(int j=i;j<arr.length-1;j++)
			{
				
			
			if(arr[i]<arr[j+1])
			{
				c=arr[j+1];
				arr[j+1]=arr[i];
				arr[i]=c;
			}
			}
			System.out.print(arr[i]+" ");
		}
		System.out.println("");
		
			
	}
	public static void bubblesort(int[] arr)
	{	
		int c;
		int d=0;
		while(d<arr.length)
		{
		for(int i=0;i<arr.length-1;i++)
		{
			if(arr[i]>arr[i+1])
			{
				c=arr[i+1];
				arr[i+1]=arr[i];
				arr[i]=c;
			}
		} d++;
		}
		for(int i=0;i<arr.length;i++)
		{
			System.out.print(arr[i]+" ");
		}
		System.out.println("");
	}
	public static void selectionsort(int[] arr)
	{ int k=0;
		for(int i=k;i<arr.length;i++)
		{
			int c=arr[i];
			for(int j=k;j<arr.length-1;j++)
			{
				if(c>arr[j+1])
				{
					int d=c;
					c=arr[j+1];
					arr[j+1]=d;
					
				}
			}
			arr[k]=c;
			k++;
		}
		for(int i=0;i<arr.length;i++)
		{
		System.out.print(arr[i]+" ");
		}
		}
}
