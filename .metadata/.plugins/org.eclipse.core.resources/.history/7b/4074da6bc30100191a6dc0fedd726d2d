package dec17;

import java.util.Scanner;

public class fibonachi {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner imp= new Scanner(System.in);
		int b = imp.nextInt();
		int c=1;
		int num=0;
		while(c<=b)
		{
			System.out.print(num+ " ");
			num+= c;
			c++;
		}
	}

}
