package page6;

public class ascii {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		  Scanner scn=new Scanner(System.in);
	        String str=scn.next();
	        String str1="";
	        for(int i=0;i<str.length();i++)
	        {
	            char ch=str.charAt(i);
	            int a=ch;
	            str1=str1+ch+a;
	        }
	        System.out.print(str1);
	    }
	}