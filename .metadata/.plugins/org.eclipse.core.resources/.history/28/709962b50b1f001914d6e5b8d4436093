package jan20;

public class fibonachii {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int c=fib1(10);
		System.out.println(c);
	}
	public static int fib1(int n){
		if(n==0 || n==1)
		{
			return n;
			
		}
		int fibone=fib1(n-1);
		int fibtwo=fib1(n-2);
		int fn=fibone+fibtwo;
		return fn;
	}

}
