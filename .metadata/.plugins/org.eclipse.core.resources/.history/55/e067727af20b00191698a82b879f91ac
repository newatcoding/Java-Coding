package dec30;

public class gettingsumof10usingdice {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}
	public static void printboardpath(int src,int dest){
		if(src==10)
		{
			System.out.println();
			return;
		}
		int ans=0;
		for(int i=1;i<=6;i++){
			ans=ans+i;
		printboardpath(i,dest); 
		
		
		}
		
		return;
	}

}
