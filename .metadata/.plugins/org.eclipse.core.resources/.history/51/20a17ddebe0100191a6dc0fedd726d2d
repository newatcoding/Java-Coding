package dec17;

import java.util.Scanner;

public class printprime {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Scanner a = new Scanner(System.in);
		int b=a.nextInt();
		int c=1;
		int d=1;
				while(c<=b)
				{
				c++;
				while(d<c)
				{ 
					d++;
				
					if(c%d==0)
					{
												break;
						}
						else
						{
							System.out.print(c);	
							break;
						}
				}
				
				}
				
	
	
