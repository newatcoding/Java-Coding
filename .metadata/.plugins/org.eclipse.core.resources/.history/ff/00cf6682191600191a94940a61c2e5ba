package jan12;

public class Sudoku {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[][] mat = { { 3, 0, 6, 5, 0, 8, 4, 0, 0 },
				{ 5, 2, 0, 0, 0, 0, 0, 0, 0 },
				{ 0, 8, 7, 0, 0, 0, 0, 3, 1 },
				{ 0, 0, 3, 0, 1, 0, 0, 8, 0 }, 
				{ 9, 0, 0, 8, 6, 3, 0, 0, 5 },
				{ 0, 5, 0, 0, 9, 0, 6, 0, 0 },
				{ 1, 3, 0, 0, 0, 0, 2, 5, 0 },
				{ 0, 0, 0, 0, 0, 0, 0, 7, 4 },
				{ 0, 0, 5, 2, 0, 6, 3, 0, 0 } };
		Sudoku(mat);
	}
	public static void Sudoku(int[][] mat)
	{
		for(int i=0;i<mat.length;i++)
		{
		for(int j=0;j<mat[0].length;j++)
		{
			if(mat[i][j]==0)
			{
				System.out.println("_");
			}
			else 
			{
			System.out.print(mat[i][j]);
			}
		}
		System.out.println(" ");
		}
	}

}
