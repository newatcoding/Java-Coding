package jan19;

public class Merging {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] one={1,3,5,6,7};
		int[] two={2,8,9};
		Merging(one,two);
	}
	public static	void Merging(int[] one ,int[] two){
		int[] arr=new int[one.length + two.length];
		int i=0; int j=0;
		for(int k=0;k<arr.length;k++)
		{
			if(one[i]>two[j])
			{
				arr[i]=one[i];
				i++;
			}
			else if(one[i]<two[j])
			{
				arr[j]=two[k];
				j++;
			}
			if(i>=one.length && j<two.length)
			{
				arr[k]=two[j];
			}
			if(i<one.length && j>=two.length)
			{
				arr[k]=two[i];
			}
		}
		for(int i1=0;i1<arr.length;i1++)
		{
			System.out.println(arr[i1]);
		}
	//	return arr;		
		
	}

}
