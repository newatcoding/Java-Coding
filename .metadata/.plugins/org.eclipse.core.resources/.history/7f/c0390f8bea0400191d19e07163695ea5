package dec21;

import java.util.Scanner;

public class rotatingarray {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] arr={0,1,2,3,4,5};
		Scanner a=new Scanner(System.in);
		int p=a.nextInt();
		int q=p-1;
		for(int i=0;i<p/2;i++)
		{
			arr[i]=arr[p-i];
		}
		for(int i=p;i<6;i++)
		{
			arr[p]=arr[6-p+i];
		}
		for(int i=0;i<6;i++)
		{
			arr[i]=arr[6-i];
			System.out.print(arr[i]+" ");
		}
	}

}
