package jan02;

public class QueenPermutaion {

	public QueenPermutaion() {
		// TODO Auto-generated constructor stub
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		QueenPermutation(new boolean[5],0,2,"");
	}
	public static void QueenPermutation(boolean[] boxes, int qno, int queens,String asf)
	{
		if(qno==queens)
		{
			System.out.println(asf);
			return;
		}
		for(int i=0;i<boxes.length;i++)
		{
			if(boxes[i]==false)
			{
				boxes[i]=true;
			}
			QueenPermutation(boxes, qno+1,queens,asf+" "+(qno+1)+"b"+i);
			boxes[i]=false;
		}
	}
}
