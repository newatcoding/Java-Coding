package dec18;

import java.util.Scanner;

public class wierdnumbers {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner n=new Scanner(System.in);
		int b=n.nextInt();
	
		int i;
		int j;
		int nc=1;
		for(i=1;i<=b;i++)
		{
			int val=1;
			for(j=1;j<=i;j++)
			{
				val=nc*(i-j)/(j+1);
				
				
				System.out.print(val+" ");
				val++;
			}
			System.out.println(" ");
		}
	}

}
