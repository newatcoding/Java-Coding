package jan19;

public class Merging {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] one={1,3,5,6,7};
		int[] two={2,8,9};
		int[] arr=Merging(one,two);
		display(arr);
	}
	public static void display(int[] arr)
	{
		for(int val:arr)
		{
			System.out.print(val+ " ");
		}
	}
	public static	int[] Merging(int[] one ,int[] two){
		int[] arr=new int[one.length + two.length];
		int i=0; int j=0;
		int k=0;
		for(k=0;k<arr.length;k++)
		{
			if(one[i]>two[j])
			{
				arr[k]=one[i];
				i++;
			}
			else if(one[i]<two[j])
			{
				arr[k]=two[j];
				j++;
			}
			if(i==one.length || j==two.length)
			{
				break;
			}
		}
		while(i<one.length){
			arr[i]=one[i];
			i++;
			k++;
		}
		while(j<two.length){
			arr[k]=one[j];
			j++;
			k++;
		}
		
		
		return arr;		
		
	}

}
